{
  "version": 2,
  "public": true,
  "github": {
    "enabled": true
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Credentials",
          "value": "true"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET,OPTIONS,PATCH,DELETE,POST,PUT"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version, Authorization, Cookie"
        },
        {
          "key": "Access-Control-Expose-Headers",
          "value": "Set-Cookie"
        }
      ]
    }
  ],
  "env": {
    "NEXTAUTH_URL": "https://app-nextjs123-xb82-6sd4gtpy2-johnmejia321s-projects.vercel.app",
    "NEXT_PUBLIC_BETTER_AUTH_URL": "https://app-nextjs123-xb82-6sd4gtpy2-johnmejia321s-projects.vercel.app/api/auth"
  }
}